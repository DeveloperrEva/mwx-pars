{
   "id":40257,
   "version":1,
   "title":"Ranobe.me",
   "host":"https://ranobe.me",
   "public_link":"https://ranobe.me/ranobe/",
   "folder":"ranobes",
   "name":"ranobes",
   "language":"ru",
   "encoding":"utf-8",
   "chapters_order":true,
   "icon_link":"https://fant.my1.ru/mwparser/mwx/img/ranobes.png",
   "self_link":"https://fant.my1.ru/mwparser/mwx/json/ranobes.json",
   "catalog_link":"https://fant.my1.ru/mwparser/mwx/catalog/ranobes.p",
   "author":{
      "name":"Veopot",
      "email":"fant-net@ya.ru"
   },
   "manga_list_complete":{
      "iterator":{
         "base_url":"https://ranobe.me/ranobe/page/",
		 
		 "append_nums": { "from": 1, "to": 100, "step": 1,  "sufix": "/"}
         
      },
      "add_manga":{
         "next":"h2 class=\"title\">",
         "title":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</a",
			"replace" : [ 
			{ "match" : "<span hidden>Новелла </span>", "text" : "" },
			{ "match" : "<span hidden>Новелла</span>", "text" : "" },
			{ "match" : "<span hidden>Ранобэ </span>", "text" : "" },
			{ "match" : "<span hidden>Ранобэ</span>", "text" : "" }
			]
         },
         "link":{
		 
            "token1":"href=\"",
            "token2":"\""
         },
		 "uniq":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</a",
			"replace" : [ 
			{ "match" : "<span hidden>Новелла </span>", "text" : "" },
			{ "match" : "<span hidden>Новелла</span>", "text" : "" },
			{ "match" : "<span hidden>Ранобэ </span>", "text" : "" },
			{ "match" : "<span hidden>Ранобэ</span>", "text" : "" }
			]
         },
         "cover":{
		 
            "token1":"class=\"cover\" style=\"background-image: url(",
            "token2":");\">"
         }
      }
   },
   "manga_complete":{

     "content_type": {
			"default": "light_novels"
		},

      "title":{
         "skip":"<head>",
         "token1":"<title>",
         "token2":"</title>"
      },
      "uniq":{
          "skip":"<head>",
         "token1":"<title>",
         "token2":"</title>"
      },
	  
      "cover":{
         "after":"class=\"FicCover\"",
         "token1":"data-src=\"",
         "token2":"\"",
"replace":{ "prefix":"https://ranobe.me" }
      },

      "author":{
         "after":"/catalog/author/",
         "token1":"\">",
         "token2":"</"
      },
      "summary":{
         "skip":"name=\"description\"",
         "token1":"content=\"",
         "token2":"\"",
         "replace":{
            "match":"<.*?>",
            "text":" "
         }
      },
      "add_genres":{
         "start":">Жанры:</",
         "end":"</div>",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            ">Для взрослых 18+<",
            ">Для взрослых 16+<",
            ">Сэйнэн<",
            ">Яой<"
         ]
      },
	  


		
	   	  "chapters_from_page":{ 
		"after": "href=\"/chapters", "token1": "/", "token2": "\"",
"replace":{ "prefix":"https://ranobe.me/chapters/" }
		},


			"chapters_from_page2": {
			"start": "class=\"block navigation ignore-select\"",
			"token1": "/chapters",
			"token2": "\"",
			"end": "class=\"activez\"",
			"replace" : { "prefix" : "https://ranobe.me" }
		},
		


      "add_chapter":{
         "start":">Содержание</",
         "end":"</body>",
		 
         "next":"class=\"FicContentsChapterName\"",
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
		 "uniq":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</"
          },
         "title":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</"
          }
      }


   },
   
   
  
	"chapter_complete": {
		"headers": {
			"cookie": "mature=c3a2ed4b199a1a15f5a5483504c7a75a7030dc4bi%3A1%3B"
		},

		"images": true,
		"add_pages": {
			"start": "property=\"og:url\"",
			"token1": "content=\"",
			"token2": "\"",
			"end": "property=\"og:description\""
		},
		"sufix": "?title=/ranobe.txt"
	},



