    "manga_search_complete": {      
        "search_link": "https://readmanga.live/search/advanced?q=$query$&offset=$page$",
        "query_type": "GET",
        "!page_multiply_by": 50,
        "page_addition_by": -1,
        "add_manga": {
            "start": "\"tiles",
            "next": "class=\"img\"",
            "link": { "after": "<h3>", "token1": "href=\"", "token2": "\"" },
            "title": { "after": "<h4 title=\"", "token1": ">", "token2": "</" },
            "cover": { "token1": "data-original='", "token2": "'" }
        }
    },
        
    "manga_list_complete": {
        "iterator": {
            "category": { "tag": "main", "name": "Последние" },
            "base_url": "https://readmanga.live/list?offset=",
            "append_nums": { "from": 0, "to": 16660, "step": 70, "last_page": {"after": "step gap", "token1": "offset=", "token2": "&amp"} }
        },
        "add_manga": {
            "start": "\"tiles",
            "next": "class=\"img\"",
            "link": { "after": "<h3>", "token1": "href=\"", "token2": "\"" },
            "title": { "after": "<h4 title=\"", "token1": ">", "token2": "</" },
            "additional_title": { "after": "img class", "token1": "title='", "token2": "'" },
            "cover": { "token1": "data-original='", "token2": "'" }
        },
        "rating_auto_inc": {
            "start": 10000,
            "inc": -1
        }
    },
	
    "manga_complete": [
        { 
            "condition": ">Сценарист:<",
            "title": { "after": "<meta itemprop=\"alternativeHeadline\"", "token1": "content=\"", "token2": "\"" },
            "additional_title": { "after": "itemprop=\"name\"", "token1": "content=\"", "token2": "\"" },
            "uniq":  { "after": "<meta itemprop=\"alternativeHeadline\"", "token1": "content=\"", "token2": "\"" },
            "author": { "skip": "<span class=\"elem_screenwriter \"", "token1": "class=\"person-link\">", "token2": "</" },
            "summary": { "after": "<meta name=\"description\"", "token1": ": ", "token2": "\"" },
            "status": { 
		  	"complete": { "start": "<b>Перевод:</b>", "end": "</p>", "values": [ "завершен", "Переведена", "Сингл"] },
                "ongoing": { "start": "<b>Перевод:</b>", "end": "</p>", "value": "продолжается" },
                "single": "<b>Сингл</b>",
		  	"licensed" : "Главы удалены",
		  	"unknown" : "нет ни одной главы" },
            "cover": { "after": "subject-cower", "token1": "src=\"", "token2": "\"" },
            "add_genre": { "token1": "/genre/", "token2": "\"" },
            "content_type": { "default": "manga" },
            "is_mature": "<a href=\"?mature=1\">",
            "add_chapter": {
                "start": "</thead>",
                "end": "</table>",
                "next": "<a href=\"",
                "link": { "token1": "<a href=\"", "token2": "\"", "replace": {"prefix": "https://readmanga.live"} },
                "title": { "after": "<a href=\"", "token1": ">", "token2": "<", "replace": { "match": "(.*)\n          \n          ", "text": "" } },  
		  	"uniq":{ "after" : "href=\"",    "token1": "/vol", "token2": "\"", "replace" : { "match" : "\\?.*", "text" : "" } }
            }
        },
        { 
            "condition": "",
            "title": { "after": "<meta itemprop=\"alternativeHeadline\"", "token1": "content=\"", "token2": "\"" },
            "additional_title": { "after": "itemprop=\"name\"", "token1": "content=\"", "token2": "\"" },
            "uniq":  { "after": "<meta itemprop=\"alternativeHeadline\"", "token1": "content=\"", "token2": "\"" },
            "author": { "skip": "<span class=\"elem_author \"", "token1": "class=\"person-link\">", "token2": "</" },
            "summary": { "after": "<meta name=\"description\"", "token1": ": ", "token2": "\"" },
            "status": { 
            "complete": { "start": "<b>Перевод:</b>", "end": "</p>", "values": [ "завершен", "Переведена", "Сингл"] },
                "ongoing": { "start": "<b>Перевод:</b>", "end": "</p>", "value": "продолжается" },
                "single": "<b>Сингл</b>",
            "licensed" : "Главы удалены",
            "unknown" : "нет ни одной главы" },
            "cover": { "after": "subject-cower", "token1": "src=\"", "token2": "\"" },
            "add_genre": { "token1": "/genre/", "token2": "\"" },
            "content_type": { "default": "manga" },
            "is_mature": "<a href=\"?mature=1\">",
            "add_chapter": {
                "start": "</thead>",
                "end": "</table>",
                "next": "<a href=\"",
                "link": { "token1": "<a href=\"", "token2": "\"", "replace": {"prefix": "https://readmanga.live"} },
                "title": { "after": "<a href=\"", "token1": ">", "token2": "<", "replace": { "match": "(.*)\n          \n          ", "text": "" } },  
            "uniq":{ "after" : "href=\"",    "token1": "/vol", "token2": "\"", "replace" : { "match" : "\\?.*", "text" : "" } }
            }
        }
    ],
   "chapter_complete": {
    	"no_404": true,
        "images": true,
        "add_servers": { 
			"start": "var servers", 
			"token1": "['", 
			"token2": "']",
			"split": "','",
			"end": ";"
		},
        "add_pages": { 
            "start": "rm_h.init(",
            "token1": "['",
            "token2": "],",
            "end": "]],",
            "replace" : { "match" : "(.*?)','(.*?)',\"(.*?)\".*",  "text" : "$1$3" }
        },
        "postfix" : "?mtr=1&mature=1"
    },
	
