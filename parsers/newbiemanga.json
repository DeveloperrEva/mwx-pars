	"manga_search_complete": {
		"headers": {
			"Accept": "*/*",
			"Referer": "https://newbie-tl.ru/",

			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		"search_link": "https://api.newbie-tl.ru/v2/projects/search?query=$query$&size=10&page=$page$",
		"query_type": "GET",


		"add_manga": {
			"start": "\"items\":[",
			"end": "],",
			"next": "\"title\":{",

			"link": {
				"before": "\"en\":\"",
				"token1": "id\":",
				"token2": ",",
				"replace": {
					"prefix": "https://api.newbie-tl.ru/v2/projects/"
				}
			},


			"title": {
				"skip": "title\"",
				"token1": "\"en\":\"",
				"token2": "\"},",
				"replace": {
					"match": "\",\"ru\":\"",
					"text": " | "
				}
			},

			"uniq": {
				"skip": "title\"",
				"token1": "\"en\":\"",
				"token2": "\","
			},

			"cover": {
				"token1": "\"large\":\"images/projects/",
				"token2": "\",",
				"replace": [{
						"prefix": "https://storage.newbie-tl.ru/images/projects/"
					},
					{
						"match": "u002F",
						"text": "/"
					}
				]
			},

			"summary": {
				"token1": "\"description\":\"",
				"token2": "\"",
				"replace": [{
						"match": "u003C",
						"text": ""
					},
					{
						"match": "<.*?>",
						"text": ""
					}
				]
			}

		}

	},



	"manga_list_complete": {
		"headers": {

			"accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9",
			"Accept-Language": "ru-RU,ru;q=0.9"
		},

		"iterator": {
			"base_url": "https://api.newbie-tl.ru/v2/projects/catalog?query=&sorting=rating&size=32&page=",

			"append_nums": {
				"from": 1,
				"to": 30
			}
		},
		"add_manga": {
			"start": "\"items\":[",
			"end": "],",
			"next": "\"title\":{",

			"link": {
				"before": "\"en\":\"",
				"token1": "id\":",
				"token2": ",",
				"replace": {
					"prefix": "https://api.newbie-tl.ru/v2/projects/"
				}
			},


			"title": {
				"skip": "title\"",
				"token1": "\"en\":\"",
				"token2": "\"},",
				"replace": {
					"match": "\",\"ru\":\"",
					"text": " | "
				}
			},

			"uniq": {
				"skip": "title\"",
				"token1": "\"en\":\"",
				"token2": "\","
			}

		},
		"rating_auto_inc": {
			"start": "20000",
			"inc": -1
		}

	},




	"manga_complete": {
