		"values": {
			"user": {
				"label": "Логин",
				"hint": "Введите свой логин",
				"kind": "text"
			},
			"password": {
				"label": "Пароль",
				"hint": "Введите свой пароль",
				"kind": "password"
			},
			"headers": {
				"Accept": "*/*"
			}
		},
