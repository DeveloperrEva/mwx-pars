			"token1": "\"en\":\"",
			"token2": "\","
		},

		"author": {
			"skip": "\"author\":{",
			"token1": "\"name\":\"",
			"token2": "\"",
			"replace": {
				"match": "<.*>",
				"text": " "
			}
		},


		"cover": {
			"token1": "\"large\":\"images/projects/",
			"token2": "\",",
			"replace": [{
					"prefix": "https://storage.newbie-tl.ru/images/projects/"
				},
				{
					"match": "u002F",
					"text": "/"
				}
			]

		},

		"read_dir": {
			"right_to_left": "\"manga\"",
			"left_to_right": "\"manhya\""
		},

		"summary": {
			"skip": "\"type\":\"",
			"token1": "\"description\":\"",
			"token2": "\",",

			"replace": [{
					"match": "u003Cp",
					"text": ""
				},
				{
					"match": "u003E",
					"text": ""
				},
				{
					"match": "u003C",
					"text": ""
				},
				{
					"match": "<.*?>",
					"text": ""
				},
				{
					"match": "\n",
					"text": " "
				},
				{
					"match": "u002Fp",
					"text": ""
				}
			]
		},


		"status": {
			"complete": {
				"values": ["complete", "Переведена", "Сингл"]
			},

			"ongoing": {
				"value": "on_going"
			}
		},
		"rating": {
			"token1": "bookmarks\":",
			"token2": ","
		},



		"add_tags": {
			"start": "genres\":[",
			"end": "],",
			"token1": "\"ru\":\"",
			"token2": "\""
		},




		"is_mature": {
			"values": ["Сэйнэн", "Эротика", "Юри", "Яой"]
		},



		"chapters_from_page": {
			"before": "\"type\":\"",
			"token1": "\"id\":",
			"token2": ",",
			"replace": [{
					"prefix": "https://api.newbie-tl.ru/v2/projects/"
				},
				{
					"sufix": "/chapters?reverse=false&page=1&size=10000"
				}
			]
		},


		"add_chapter": {
			"start": "items",
			"end": "]",
			"next": "{\"",

			"link": {                       
				"token1": "id\":",
				"token2": ",\"",
				"replace": [
				    {
						"prefix": "https://api.newbie-tl.ru/v2/chapters/"
					},	
					{
						"sufix": "/pages/"
					}
				]
			},

		
			"uniq": {
                "token1": "\"tom\":",
				"token2": ".0,\"hearts\"",
				"replace": [{
						"match": "\"name\":(.*?),",
						"text": " "
					},
					{
						"match": "\"number\":",
						"text": "Глава "
					},
					{
						"match": "\"name\":null,",
						"text": " "
					},
					{
						"prefix": "Том "
					}
				]
			},

			"title": {
                "token1": "\"tom\":",
				"token2": ".0,\"hearts\"",
				"replace": [{
						"match": "\"name\":(.*?),",
						"text": " "
					},
					{
						"match": "\"number\":",
						"text": "Глава "
					},
					{
						"match": "\"name\":null,",
						"text": " "
					},
					{
						"prefix": "Том "
					}
				]
			}
		}

	},


