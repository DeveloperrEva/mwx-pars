{
	"id": 30435,
	"old_ids": [20569],
	"version": 16,
	"title": "MangaDex (API)",
	"host": "https://api.mangadex.org/",
	"public_link": "https://mangadex.org/",
	"folder": "mangadex",
	"name": "mangadex",
	"language": "en",
	"chapters_order": false,
	"skip_catalog_test": false,
	"icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/mangadex.png",
	"self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/mangadex.json",
	"self_link2": "https://fant.my1.ru/mwparser/mwx/json/mangadex.json",
	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/mangadex.zip",
	"file_type": "sqlite",
	"author": {
		"name": "Veopot & Lomik_XP",
		"email": "fant-net@ya.ru"
	},
	"authorization_complete": {
		"logo": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/mangadex.png",
		"registration": "https://mangadex.org/signup",
		"expires": 31449600000,
		"values": {
			"two_factor": "",
			"remember_me": "1",
			"login_username": {
				"label": "Login",
				"kind": "login"
			},
			"login_password": {
				"label": "Password",
				"kind": "password"
			},
			
			"mangadex_h_toggle": {
				"forward": "cookie",
				"value": "1"
			}
			
		},
		"request": {
			"method": "post",
			"url": "https://mangadex.org/ajax/actions.ajax.php?function=login",
			"headers": {
				"x-requested-with": "XMLHttpRequest"
			}
		},
		"response_check": {
			"cookies": {
				"mangadex_rememberme_token": "@\\w+",
				"mangadex_session": "@\\w+"
			},
			"headers": {
				"Set-Cookie": "@mangadex_rememberme_token=\\w+"
			}
		}
	},
	
	"manga_search_complete": {
		
		"search_link": "https://api.mangadex.org/manga?limit=32&offset=$page$&includes[]=cover_art&contentRating[]=safe&contentRating[]=suggestive&contentRating[]=erotica&title=$query$&order[relevance]=desc",
		"query_type": "GET",


		"add_manga": {
			"start": "response",
			"end": "total",
			
			"next": "\"title\":{",
			
			"title": {
			"token1": "\"en\":\"",
			"token2": "\""
			},
			
			"uniq": {
			    "before": "\"en\":\"",
			    "token1": "\"id\":\"",
				"token2": "\""
			},
			
			"link": {
				"before": "\"en\":\"",
			    "token1": "\"id\":\"",
				"token2": "\"",
			    "replace": [
			        {"prefix": "https://api.mangadex.org/manga/"},
			        {"sufix": "?includes[]=artist&includes[]=author&includes[]=cover_art&"}
			                ]
			},

			"cover": {
			"after": "cover_art\",",
			"token1": "fileName\":\"",
			"token2": "\"",
			"replace": { "prefix": "https://uploads.mangadex.org/covers/"}
			},
			
            "add_tags": {
			"start": "\"tags\":[",
			"end": "createdAt",
			"token1": "\"en\":\"",
			"token2": "\"",
			"replace": { "match": "<.*?>",	"text": ""}
		    },
			
		
			"summary": {
				"token1": "\"description\":{\"en\":\"",
				"token2": "\"",
				"replace": [{
						"match": "u003C",
						"text": ""
					},
					{
						"match": "<.*?>",
						"text": ""
					}
				]
			}

		}

	},



	"manga_list_complete": {
	"headers": {
			"cookie": "mangadex_h_toggle=1"
		},
		
		"iterator": {
			"base_url": "https://api.mangadex.org/manga?limit=32&includes[]=cover_art&contentRating[]=safe&contentRating[]=suggestive&contentRating[]=erotica&order[followedCount]=desc&offset=",
			"append_nums": {
				"from": 0,
				"to": 10000,
				"step": 32
			}
		},
		"add_manga": {
			"next": "\"title\":{",
			
			"title": {
			"token1": "\"en\":\"",
			"token2": "\""
			},
			"uniq": {
			    "before": "\"en\":\"",
			    "token1": "\"id\":\"",
				"token2": "\""
			},
			"link": {
				"before": "\"en\":\"",
			    "token1": "\"id\":\"",
				"token2": "\"",
			    "replace": [
			        {"prefix": "https://api.mangadex.org/manga/"},
			        {"sufix": "?includes[]=artist&includes[]=author&includes[]=cover_art&"}
			                ]
			}
		}
	},
	"manga_complete": {
		
		"title": {
			"after": "\"title\":{",
			"token1": "\"en\":\"",
			"token2": "\""
		},
		"uniq": {
			"after": "\"title\":{",
			"token1": "\"en\":\"",
			"token2": "\""
		},
		
		
		"author": {
			"skip": "type\":\"author\"",
			"token1": "\"name\":\"",
			"token2": "\""
		},
		"summary": {
			"after":"\"description\":{",
			"token1":"\"en\":\"",
			"token2": "\""
		},
		"status": {
			"complete": "\"completed\",",
			"ongoing": "\"ongoing\","
		},
		"cover": {
			"after": "cover_art\",",
			"token1": "fileName\":\"",
			"token2": "\"",
			"replace": [
                        { "match" : "111111",  "text" : "222222" },
						{ "prefix": "https://uploads.mangadex.org/covers/%%page%%/" }
						]
		},
		
		"is_mature": "<span class='badge badge-danger ml-1'>H</span>",
		
		"add_genres": {
			"start": "\"tags\":[",
			"end": "createdAt",
			"token1": "\"en\":\"",
			"token2": "\"",
			"replace": { "match": "<.*?>",	"text": ""}
		},
		
		"add_tags": {
			"start": "\"tags\":[",
			"end": "createdAt",
			"token1": "\"en\":\"",
			"token2": "\"",
			"replace": { "match": "<.*?>",	"text": ""}
		},
		
		"chapters_from_page": {
			"after": "data\":{",
			"token1": "\"id\":\"",
			"token2": "\",",
			"replace": [
			    {
					"prefix": "https://api.mangadex.org/manga/"
				},
				{
					"sufix": "/feed?limit=500&offset=1"
				}
			]
		},
		
			
		"add_chapter": {
			"start": "response",
			"next": "type\":\"chapter\",",
			
			"link": {
				"before": "chapter\":",
				"token1": "\"id\":\"",
				"token2": "\",",
				"replace": {
					"prefix": "https://api.mangadex.org/chapter/"
				}
			},
			"title": {
				"token1": "chapter\":\"",
				"token2": "\",",
				"replace": {
					"sufix": "&nbsp;Language&nbsp;%%chapter_lang%%"
				}
			},
			
			"lang": {"token1": "translatedLanguage\":\"", "token2": "\",", "replace": [
				{ "match": "\\ben\\b", "text": "English" },
				{ "match": "\\bja\\b", "text": "Japanese" },
				{ "match": "\\bpl\\b", "text": "Polish" },
				{ "match": "\\bsh\\b", "text": "SerboCroatian" },
				{ "match": "\\bnl\\b", "text": "Dutch" },
				{ "match": "\\bit\\b", "text": "Italian" },
				{ "match": "\\bru\\b", "text": "Russian" },
				{ "match": "\\bde\\b", "text": "German" },
				{ "match": "\\bhu\\b", "text": "Hungarian" },
				{ "match": "\\bfr\\b", "text": "French" },
				{ "match": "\\bfi\\b", "text": "Finnish" },
				{ "match": "\\bvi\\b", "text": "Vietnamese" },
				{ "match": "\\bel\\b", "text": "Greek" },
				{ "match": "\\bbg\\b", "text": "Bulgarian" },
				{ "match": "\\bes\\b", "text": "SpanishSpain" },
				{ "match": "\\b(pt-BR|pt-br)\\b",  "text": "PortugueseBrazil" },
				{ "match": "\\bpt\\b", "text": "PortuguesePortugal" },
				{ "match": "\\bsv\\b", "text": "Swedish" },
				{ "match": "\\bar\\b", "text": "Arabic" },
				{ "match": "\\bda\\b", "text": "Danish" },
				{ "match": "\\b(zh-Hans|zh)\\b", "text": "ChineseSimp" },
				{ "match": "\\bbn\\b", "text": "Bengali" },
				{ "match": "\\bro\\b", "text": "Romanian" },
				{ "match": "\\bcs\\b", "text": "Czech" },
				{ "match": "\\bmn\\b", "text": "Mongolian" },
				{ "match": "\\btr\\b", "text": "Turkish" },
				{ "match": "\\bid\\b", "text": "Indonesian" },
				{ "match": "\\bko\\b", "text": "Korean" },
				{ "match": "\\b(es-419|es-la)\\b", "text": "SpanishLTAM" },
				{ "match": "\\bfa\\b", "text": "Persian" },
				{ "match": "\\bms\\b", "text": "Malay" },
				{ "match": "\\bth\\b", "text": "Thai" },
				{ "match": "\\bca\\b", "text": "Catalan" },
				{ "match": "\\b(fil|tl)\\b", "text": "Filipino" },
				{ "match": "\\b(zh-Hant|zh-hk)\\b", "text": "ChineseTrad" },
				{ "match": "\\buk\\b", "text": "Ukrainian" },
				{ "match": "\\bmy\\b", "text": "Burmese" },
				{ "match": "\\blt\\b", "text": "Lithuanian" },
				{ "match": "\\bhe\\b", "text": "Hebrew" },
				{ "match": "\\bhi\\b", "text": "Hindi" },
				{ "match": "\\bno\\b", "text": "Norwegian" }
			]}
			
		}
	},
	"chapter_complete": {
		"images": true,
		"use_servers_only": true,
		"prefix": {
			"after": "\"hash\"",
			"token1": ":\"",
			"token2": "\"",
			"stop": ",",
			"replace": [{
					"prefix": "https://uploads.mangadex.org/data/"
				},
				{
					"sufix": "/"
				}
			]
		},
		
		
		"add_pages": {
			"start": "\"data\":",
			"token1": "[\"",
			"token2": "\"],",
			"split": "\",\"",
			"end": "],"
		}
	},
	"test": [{
			"proc": "manga_list_complete",
			"testpages": "3",
			"assert": {
				"mangas": [{
					"title": "@^(\\w+)",
					"link": "@https:"
				}],
				"mangas_count": ">=50"
			}
		},
