{
   "id":40228,
   "version":12,
   "title":"MangaPoisk",
   "host":"https://mangapoisk.ru",
   "public_link":"https://mangapoisk.ru/manga-list",
   "folder":"mangapoisk",
   "name":"mangapoisk",
   "language":"ru",
   "encoding":"utf-8",
   "chapters_order":false,
   
   "icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/mangapoisk.png",
	"self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/mangapoisk.json",
	"self_link2": "https://fant.my1.ru/mwparser/mwx/json/mangapoisk.json",
	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/mangapoisk.zip",
    "file_type": "sqlite",
	
   
   "author":{
      "name":"Veopot",
      "email":"fant-net@ya.ru"
   },
   "manga_list_complete":{
      "iterator":{
         "base_url":"https://mangapoisk.ru/manga?sortBy=popular&page=",
         "append_nums":{
            "from":0,
            "to":158,
            "step":1
         }
      },
      "add_manga":{
         "next":"class=\"post-description\">",
		 
         "title":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
         "cover":{
		    "before":"href=\"",
            "token1":"data-src=\"",
            "token2":"\""
         }
      },
		"rating_auto_inc": {
			"start": "20000",
			"inc": -1
		}
   },
   
   
   "manga_search_complete": {		
        "search_link":"https://mangapoisk.ru/search?q=$query$&page=$page$",
		"query_type": "GET",
		
		 "add_manga":{
		 
         "next":"<article class=\"flex-item",
		 
         "title":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
		 
		 "summary": { "skip":"href=\"", "token1": "<p class=\"card-text\">", "token2": "</p>",
                "replace":{"match":"<.*?>", "text":""} },
				
         "cover":{
		    "token1":"data-src=\"",
            "token2":"\""
         }
      }
		},	
		
		
   
   "manga_complete":[
   
    { "condition": "/chaptersList?",
	   "headers": {
			"User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/88.0.3987.163 Safari/537.36",
			"Referer": "https://mangapoisk.ru"
		},
      "title":{
         
         "token1":"class=\"post-name\">",
         "token2":"</h1>" ,
         "replace":{"match":"<.*?>","text":""} },
      
      "uniq":{
         "skip":"class=\"row\">",
         "token1":"class=\"post-name\">",
         "token2":"</"
      },
	  
	  
      "cover":{
        "skip":"class=\"row\">",
         "token1":"src=\"",
         "token2":"\""
      },
      
	  "status": { 
		  	"complete": { 
			 "start": ">Статус:<",
             "end": ">Жанр:<",
			   "values": [ "Завершена", "Переведена", "Сингл"] },
			
            "ongoing": { 
			"start": ">Статус:<",
             "end": ">Жанр:<",
                "value": "Выпускается" }
			 },
			 
			 
      "summary":{
         "skip":">Описание:<",
         "token1":"class=\"manga-description entry\">",
         "token2":"</div>",
         "replace":{
            "match":"<.*?>",
            "text":""
         }
      },
      "add_genres":{
         "start":">Жанр:<",
         "end":">Год:<",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            "эротика",
            "сэйнэн",
            "юри",
			"этти",
            "яой"
         ]
      },
	  
	"chapters_from_pages": { 
		"start": "class=\"chapters-infinite-pagination",
		 "end": "</ul>",
		"after": "class=\"page-link",
		"token1": "\">",
		"token2": "</", 
		"replace" :  { "prefix" : "%%url%%/chaptersList?page=" } 
		
		},
	  
	  
	  
		
		"add_chapter":{
        "start": "<head>",
		 "end": "class=\"chapters-infinite-pagination\"",
        
		 
         "next": "chapter-item",
		 
		                 
         "link":{
			
            "token1":"href=\"",
            "token2":"\"",
			"replace": {"prefix" : "https://mangapoisk.ru"}
         },
		 
		"uniq":{
            "skip":"href=\"",
            "token1":"/chapter/",
            "token2":"\""
        },
		 
        "title":{
            "skip":"href=\"",
            "token1":"class=\"chapter-title\">",
            "token2":"</a>",
			"replace": { "match": "</span>",	"text": ""}
        }
      }
	  
	  
   },
   { "condition": "",
	   "headers": {
			"User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/88.0.3987.163 Safari/537.36",
			"Referer": "https://mangapoisk.ru"
		},
      "title":{
         
         "token1":"class=\"post-name\">",
         "token2":"</h1>" ,
         "replace":{"match":"<.*?>","text":""} },
      
      "uniq":{
         "skip":"class=\"row\">",
         "token1":"class=\"post-name\">",
         "token2":"</"
      },
	  
	  
      "cover":{
        "skip":"class=\"row\">",
         "token1":"src=\"",
         "token2":"\""
      },
      
	  "status": { 
		  	"complete": { 
			 "start": ">Статус:<",
             "end": ">Жанр:<",
			   "values": [ "Завершена", "Переведена", "Сингл"] },
			
            "ongoing": { 
			"start": ">Статус:<",
             "end": ">Жанр:<",
                "value": "Выпускается" }
			 },
			 
			 
      "summary":{
         "skip":">Описание:<",
         "token1":"class=\"manga-description entry\">",
         "token2":"</div>",
         "replace":{
            "match":"<.*?>",
            "text":""
         }
      },
      "add_genres":{
         "start":">Жанр:<",
         "end":">Год:<",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            "эротика",
            "сэйнэн",
            "юри",
			"этти",
            "яой"
         ]
      },
	  
	
	  
	  
	  
		
		"add_chapter":{
        "start": "<head>",
		 "end": "class=\"chapters-infinite-pagination\"",
        
		 
         "next": "chapter-item",
		 
		                 
         "link":{
			
            "token1":"href=\"",
            "token2":"\"",
			"replace": {"prefix" : "https://mangapoisk.ru"}
         },
		 
		"uniq":{
            "skip":"href=\"",
            "token1":"/chapter/",
            "token2":"\""
        },
		 
        "title":{
            "skip":"href=\"",
            "token1":"class=\"chapter-title\">",
            "token2":"</a>",
			"replace": { "match": "</span>",	"text": ""}
        }
      }
	  
	  
   }
   ],
   "chapter_complete":{
    "add_numeration": true,
        "headers": {
			"Accept": "*/*",
			"Referer": "https://mangapoisk.ru/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
     
      "images":true,
      "add_pages":{
         "start":"class=\"text-center\">",
         "token1":"\"https://static2.mangapoisk.ru/",
         "token2":"\"",
         "end":"<div class=\"row text-center\">",
         "replace":{
            "prefix":"https://static2.mangapoisk.ru/"
         }
      }
   },
   "test":[
      {
         "proc":"manga_list_complete",
         "assert":{
            "mangas":[
               {
                  "title":"Ванпанчмен",
                  "link":"https://mangapoisk.ru/manga/vanpanchmen",
                  "uniq":"vanpanchmen"
               }
            ],
            "mangas_count":">=14"
         }
      },
      {
