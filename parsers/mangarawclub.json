{
    
    "id": 30469,
	
	"version": 5,
	
	"title": "Manga Raw Club",
	"host": "https://www.manga-raw.club/",
	"public_link": "https://www.manga-raw.club/manga",
	"folder": "mangarawclub",
	"name": "mangarawclub",
	"language": "en",
	
	"icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/mangarawclub.png",
	"self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/mangarawclub.json",
	"self_link2": "https://fant.my1.ru/mwparser/mwx/json/mangarawclub.json",
	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/mangarawclub.zip",
    "file_type": "sqlite",
	



	"manga_search_complete": {		
		"search_link": "https://www.manga-raw.club/search/?search=$query$",
		"query_type": "GET",
		"add_manga": {
			"next": "class=\"novel-item\">",
			"link":  { "token1": "href=\"", "token2": "\"",
				"replace": {"prefix": "https://www.manga-raw.club"}
			},
			
			"title": { "token1": "title=\"", "token2": "\"" },
			"uniq": { "token1": "href=\"/manga/", "token2": "/\"" },
			 
			"summary": { "token1": "<div class=\"summary truncate-fade\" title=\"", "token2": "\"",
                "replace":{"match":"<.*?>", "text":""} },
			
			
			
			"cover": { "token1": "data-src='", "token2": "'",
				"replace": {"prefix": "https://www.manga-raw.club"} }
			
		}
	},

	"manga_list_complete": {
		"iterator": {
			"base_url": "https://www.manga-raw.club/browse/?results=",
			"append_nums": { "from": 0, "to": 150, "step": 1, "sufix": "&filter=views" }
		},
		"add_manga": {
			"next": "class=\"novel-item\">",
			"link":  { "token1": "href=\"", "token2": "\"",
				"replace": {"prefix": "https://www.manga-raw.club"}
			},
			
			"title": { "token1": "title=\"", "token2": "\"" },
			"uniq": { "token1": "href=\"/manga/", "token2": "/\"" }
		},
		"rating_auto_inc": {
			"start": "20000",
			"inc": -1
		}
	},

	"manga_complete": {
		"title":       { "after": "<h1 itemprop=\"name\"", "token1":"\">", "token2": "</" },
		"uniq":        { "after": "property=\"og:url\"", 
		 "token1":"content=\"https://www.manga-raw.club/manga/", "token2": "/\"" },
		 
		"author":      { "token1":"<span itemprop=\"author\">", "token2": "</" },
		
		"summary":     { "skip": "<h4>Summary</h4>", "token1": ">", "token2": "</div>" ,
         "replace":{"match":"<.*?>","text":""} 
		},
		
		"status":      { 
		
		"single": ">Сингл</", 
		"complete": "<strong class=\"completed\">",
		"ongoing": "<strong class=\"ongoing\">"
		 },
		
		"is_mature": {
			"start": ">Categories<",
			"end": "</ul>",
			"values": ["Mature", "Seinen", ">эротика<"]
		},
		
		
		
		"cover":       { "after": "property=\"og:image\"", "token1": "content=\"", "token2": "\"" },
		
		"add_genre":   { "after": "href=\"/browse/?genre=", "token1": "\">", "token2": "</" },
		
		"add_chapter": {
			"next":  "<li data-chapterno",
			"link":  { "token1": "href=\"", "token2": "\"",
				"replace": {"prefix": "https://www.manga-raw.club"}
			  },
			"title": { "skip" : "href=\"",  "token1": "class=\"chapter-title\">", "token2": "</" },
			"uniq": { "skip" : "href=\"",  "token1": "class=\"chapter-title\">", "token2": "</" }
		}   
	},
	
	"chapter_complete": {
		"add_images": {
			"start": "<div style=\"font-size: 0;\">",
			"token1": "<img src=\"",
			"token2": "\"",
			"end": ">Comments</"
		}
	},

	"test": [
		{
			"proc" : "manga_list_complete",
			
			"assert" : {
				"mangas" : [
            { "title" : "Alam", 
            "link" : "https://www.manga-raw.club/manga/alam/",
			"uniq" : "alam" }
				],
				"mangas_count" : ">=50"
			}
		},
		{
			"proc" : "manga_complete",
			"link" : "https://www.manga-raw.club/manga/onepunch-man/",
			"title" : "Onepunch-Man",
			"assert" : {
				"author" : "@One , Murata Yuusuke",
				"cover" : "@https://www.manga-raw.club/media/manga_covers/cover_2_C7YYSFm.jpg",
				"summary" : "@^(One punch-Man imitates the)",
				"genres" : "@comedy",
				"status" : "ongoing",
				"genres_count" : ">=5",
				"chapters_count" : ">=100",
				"chapters" : [
					{
            "folder": "147-eng-li/",
            "link": "https://www.manga-raw.club/reader/en/onepunch-man-chapter-147-eng-li/",
            "title": "147-eng-li"
        },
        {
            
            "link": "https://www.manga-raw.club/reader/en/onepunch-man-chapter-146-eng-li/",
             "title": "146-eng-li"
        }
				]
			}
		},
		{
			"proc" : "chapter_complete",
			"link" : "https://www.manga-raw.club/reader/en/onepunch-man-chapter-146-eng-li/",
			"assert" : {
				"pages" : [
		{"image": "https://toy.novel-fast.club/cdn_mangaraw/onepunch-man/chapter-146/01.jpg"},
        {"image": "https://toy.novel-fast.club/cdn_mangaraw/onepunch-man/chapter-146/02.jpg"},
        {"image": "https://toy.novel-fast.club/cdn_mangaraw/onepunch-man/chapter-146/03.jpg"},
        {"image": "https://toy.novel-fast.club/cdn_mangaraw/onepunch-man/chapter-146/04.jpg"}
				],
				"pages_count": "30"
			}
		}
	]
}
