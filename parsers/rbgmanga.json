{
	"id": 40155,
	"version": 55,
	"title": "Re-Manga!",
	"host": "https://remanga.org",
	"public_link": "https://remanga.org/manga",
	"folder": "rbgmanga",
	"name" : "rbgmanga",
	"chapters_order": true,
	"language": "ru",
	"encoding": "utf-8",
	"icon_link": "https://fant.my1.ru/mwparser/mwx/img/rbgmanga.png",
	"self_link": "https://fant.my1.ru/mwparser/mwx/json/rbgmanga.json",
	"catalog_link": "https://fant.my1.ru/mwparser/mwx/catalog/rbgmanga.p",
    "author": {"name": "Veopot","email": "fant-net@ya.ru"},

   

 "manga_search_complete": {
	    "headers": {
			"Accept": "*/*",
			"Referer": "https://remanga.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		"search_link": "https://api.remanga.org/api/search/?count=10&page=$page$&query=$query$",
		"query_type": "GET",
		
		
		"add_manga":  {
		    "start": "content\"",
			"end": "props\"",
            "next": "bookmark_type\"",
			
			"link":  { "before": "bookmark_type\"",
			"token1": "dir\":\"", "token2": "?",
			"replace":{"prefix": "https://remanga.org/manga/"} },
			
			"cover":  { "before": "bookmark_type\"",
			"token1": "cover_mid\":\"", "token2": "\"",
			"replace": {"prefix": "https://api.remanga.org/media/"}},
            
			"title": { "before": "bookmark_type\"",
			"token1": "en_name\":\"", "token2": "\",\"dir",
			"replace": { "match" : "\",\"rus_name\":\"",  "text" : ", " }},
			
			"uniq": { "before": "bookmark_type\"",
			"token1": "en_name\":\"", "token2": "\""}
			
			}
		
		},

	"manga_list_complete": {
	   "headers": {
			"Accept": "*/*",
			"Referer": "https://remanga.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},

	  "iterator": {
		"base_url": "https://api.remanga.org/api/search/catalog/?ordering=-rating&count=30&page=",
		"append_nums": { "from": 1, "to": 509 }
		},
		"add_manga": {
		    "start": "content\"",
			"end": "props\"",
            "next": "bookmark_type\"",
			
			"link":  { "before": "bookmark_type\"",
			"token1": "dir\":\"", "token2": "?",
			"replace":{"prefix": "https://remanga.org/manga/"} },
			
			"cover":  { "before": "bookmark_type\"",
			"token1": "cover_mid\":\"", "token2": "\"",
			"replace": {"prefix": "https://api.remanga.org/media/"}},
            
			"title": { "before": "bookmark_type\"",
			"token1": "en_name\":\"", "token2": "\",\"dir",
			"replace": { "match" : "\",\"rus_name\":\"",  "text" : ", " }},
			
			"uniq": { "before": "bookmark_type\"",
			"token1": "en_name\":\"", "token2": "\""}
			
			},
		"rating_auto_inc": {
			"start": "20000",
			"inc": -1
		}
		
	},
   
	
	
	
	"manga_complete": {
	    "headers": {
			"Accept": "*/*",
			"Referer": "https://remanga.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
"link_corrector" : [ 			
{ "match" : "https://remanga.org/manga/", 
"text" : "https://api.remanga.org/api/titles/" } 		],


	    "title":{ 
           "after":"property=\"og:title\"",
             "token1":"content=\"",
             "token2":"\"",
              "replace": [
                { "match" : "Читать мангу","text":"" },
				{ "match" : "онлайн","text":""}
				]},
		
		"uniq":{ 
		    "token1":"en_name\":\"",
			"token2": "\","},
		
	    "cover":{
           "after":"property=\"og:image\"",
           "token1":"content=\"",
           "token2":"\""
        },
		
		
		
		
		"summary":{ 
		
		"token1":"description\":\"", "token2": "\",",
"replace": [
 { "match" : "u003Cp",  "text" : "" },
{ "match" : "u003E",  "text" : "" },
{ "match" : "u003C",  "text" : "" },
{ "match" : "u002Fp",  "text" : "" }
						]
},
		
		"status": { 
		  	"complete": { 
			 "start": "content",
             "end": "similarManga",
			   "values": [ "Закончен", "Переведена", "Сингл"] },
			
            "ongoing": { 
			 "start": "content",
             "end": "similarManga",
                "value": "Продолжается" }
			 },
		
		"chapters":{ 
		"start": "content",
        "end": "similarManga",
		"token1":"total_chapters\":", "token2":","},
		
		
		"add_genres":   {
        "start": "genres\":",
		"end": "],",
		"token1": "name\":\"",
		"token2": "\""
		},
		
		
		"is_mature":   { "values": [">Сэйнэн<", ">Эротика<", ">Юри<", ">Яой<"] },
		
		
		"chapters_from_page": 
		
		{ 
		"after": "branches\"", "token1": "id\":", "token2": ",",
		"replace" : { "prefix" : "https://api.remanga.org/api/titles/chapters/?branch_id=" }
		},
		
		
		"add_chapter": {
            "start": "content\"",
            "end": "],",
            "next": "\"is_paid\":false,",
			
            "link": {
			 "before": "\"is_paid\":false,",
			 "token1": "id\":",
                "token2": ",",
                "replace": {
                        "prefix": "https://api.remanga.org/api/titles/chapters/"
                }                  
            },
			"uniq": {
                 "before": "\"is_paid\":false,",
			    "token1": ",\"to",
                "token2": "\",\"name\"",
				"replace": [
                        { "match" : "me\":",  "text" : "Том_" },
						{ "match" : ",\"chapter\":\"",  "text" : "_Глава_" }
						]
            },
            
            "title": {
			     "before": "\"is_paid\":false,",
                "token1": ",\"to",
                "token2": "\",\"name\"",
				"replace": [
                        { "match" : "me\":",  "text" : "Том " },
						{ "match" : ",\"chapter\":\"",  "text" : " Глава " }
						
						]
            }
        }
	 
},


	"chapter_complete": {
	    "add_numeration": true,
	    "headers": {
			"Accept": "*/*",
			"Referer": "https://remanga.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		"add_images": {
			"start": "content",
			"token1": "link\":\"",
			"token2": "\"",
			 "end": "props"
		}
	},
	"test": [
		{
			"proc" : "manga_list_complete",
			   "assert" : {
				"mangas" : [
					{ "title" : "Да здравствует призыв!",
					 "link" : "https://remanga.org/manga/long_live_summons/",
					  "uniq" : "long_live_summons" }
				],
				"mangas_count" : ">=20"
			}
		},
		{
			"proc" : "manga_complete",
			"link" : "https://remanga.org/manga/evil-ladys-hero",
			"title" : "Switch Girl",
			"assert" : {
				"cover" : "https://remanga.org/media/titles/switch_girl/l_head.png",
				"summary" : "@Тамия Ника",
				"genres" : "@comedy",
				"status" : "ongoing",
				"genres_count" : ">=4",
				"chapters_count" : ">=40",
				"chapters" : [
			{
            "folder": "1-1/",
            "link": "https://remanga.org/manga/switch_girl/vol1/1",
            "title": "Том 1 Глава 1"
        },
        {
            "folder": "1-2/",
            "link": "https://remanga.org/manga/switch_girl/vol1/2",
            "title": "Том 1 Глава 2"
        }
				]
			}
		},
		{
			"proc" : "chapter_complete",
			"link" : "https://api.remanga.org/api/titles/chapters/259605",
			"title" : "Том 1 Глава 1",
			"assert" : {
				"pages" : [
{"image": "https://img.remanga.org/images/switch_girl/1-1/bg.jpg"},
{"image": "https://img.remanga.org/images/switch_girl/1-1/_____________________.jpg"},
{"image": "https://img.remanga.org/images/switch_girl/1-1/photo-1223836-xl.jpg"}
				],
				"pages_count": "35"
			}
		}
	]
}
