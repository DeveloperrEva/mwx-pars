   "manga_list_complete":{
      "iterator":{
         "base_url":"https://mangakatana.com/manga/page/",
         "append_nums":{
            "from":1,
            "to":1279,
            "step":1,
			"sufix": "?filter=1&include_mode=and&order=numc"
         }
      },
      "add_manga":{
         "next":"class=\"media\">",
         "title":{
            "skip":"class=\"title\">",
            "token1":"\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
         "cover":{
		    
            "token1":"src=\"",
            "token2":"\""
         }
      },
		"rating_auto_inc": {
			"start": "30000",
			"inc": -1
		}
   },
   
   
   "manga_search_complete": {
 "search_link":"https://mangakatana.com/page/$page$?search=$query$&search_by=book_name",

		"query_type": "GET",
		 "add_manga":{
         "next":"class=\"media\">",
         "title":{
            "skip":"class=\"title\">",
            "token1":"\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
         "cover":{
		    
            "token1":"src=\"",
            "token2":"\""
         }
      }
		},	
		
		
   
   "manga_complete":{
      "title":{ 
           "after":"property=\"og:title\"",
             "token1":"content=\"",
             "token2":"\""},
      
      "uniq":{ 
           "after":"property=\"og:title\"",
             "token1":"content=\"",
             "token2":"\""},
	  
	  
      "cover":{ 
           "after":"property=\"og:image\"",
             "token1":"content=\"",
             "token2":"\""},
      
	  "status": { 
	  "single": "Сингл<",
	  "complete": "class=\"d-cell-small value status completed\">Completed</",
	  "ongoing": "class=\"d-cell-small value status ongoing\">Ongoing</",
	  "empty": "Нет глав<" },
			 
      "summary":{ 
           "after":"property=\"og:description\"",
             "token1":"content=\"",
             "token2":"\""},
	  
	  "author": { "after": "href=\"https://mangakatana.com/author/", "token1":">", "token2": "</" },	 
			 
		 
      "add_genres":{
         "start":">Genres:</",
         "end":">Status:</",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            ">Adult<",
            ">Sexual violence<",
            ">Seinen<",
			">Yuri<",
            ">Yaoi<"
         ]
      },
	  
	  
		
		"add_chapter":{
		
         "start":"class=\"aff_items uk-overflow-container\">",
         "end":"id=\"hot_book\">",
		 
         "next":"class=\"chapter\">",
		 
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
		 "uniq":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</a>"
         },
         "title":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</a>"
         }
      
   }
   },
   
   
   "chapter_complete":{
    "add_numeration": true,
        "headers": {
			"Accept": "*/*",
			"Referer": "https://mangakatana.com/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
     
      "images":true,
      "add_pages":{
         "start":"var ytaw",
         "token1":"=['",
         "token2":"',]",
		 "split" : "','",
         "end":"id=\"copyright\">"
      }
   },
