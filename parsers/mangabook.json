{
   "id":40219,
   "version":19,
   "title":"MangaBook",
   "host":"https://mangabook.org",
   "public_link":"https://mangabook.org/manga-list",
   "folder":"mangabook",
   "name":"mangabook",
   "language":"ru",
   "encoding":"utf-8",
   "chapters_order":false,
   "icon_link":"https://fant.my1.ru/mwparser/mwx/img/mangabook.png",
   "self_link":"https://fant.my1.ru/mwparser/mwx/json/mangabook.json",
   "catalog_link":"https://fant.my1.ru/mwparser/mwx/catalog/mangabook.p",
   "author":{
      "name":"Veopot",
      "email":"fant-net@ya.ru"
   },
   "manga_list_complete":{
      "iterator":{
         "base_url":"https://mangabook.org/manga-list?page=",
         "append_nums":{
            "from":0,
            "to":500,
            "step":1
         }
      },
      "add_manga":{
         "next":"<article class=\"short clearfix\"",
         "title":{
            "skip":"href=\"",
            "token1":"title=\"",
            "token2":"\""
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
         "cover":{
            "token1":"src='",
            "token2":"'"
         }
      }
   },
   "manga_complete":{
      "title":{
         "skip":"id=\"fmain\">",
         "token1":"h1>",
         "token2":"</h1"
      },
      "uniq":{
         "skip":"id=\"fmain\">",
         "token1":"h1>",
         "token2":"</h1",
         "replace":{
            "match":"/.*",
            "text":""
         }
      },
      "cover":{
         "after":"class=\"img-responsive",
         "token1":"src='",
         "token2":"'"
      },
      "author":{
         "after":"/manga-list/author/",
         "token1":"\">",
         "token2":"</"
      },
      "summary":{
         "skip":">Сюжет:</div>",
         "token1":">",
         "token2":"</div>",
         "replace":{
            "match":"<.*?>",
            "text":""
         }
      },
      "add_genres":{
         "start":">Категории:</",
         "end":"</li>",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            "Эта Manga может содержать сцены насилия не допустимые для людей младше 18 лет",
            ">эротика<",
            ">юри<",
            ">яой<"
         ]
      },
      "add_chapter":{
         "start":">Список глав</",
         "end":"КОММЕНТАРИИ</",
         "next":"<h5>",
         "link":{
            "token1":"href=\"",
            "token2":"\"",
			"replace": {"sufix": "/1"}
         },
		 "uniq":{
            "skip":"href=\"",
            "token1":">",
            "token2":"</",
            "replace":{
               "match":"№",
               "text":" "
            }
         },
         "title":{
            "skip":"href=\"",
            "token1":">",
            "token2":"</",
            "replace":{
               "match":"№",
               "text":" "
            }
         }
      }
   },
   "chapter_complete":{
        "headers": {
			"Accept": "*/*",
			"Referer": "https://mangabook.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
     
      "images":true,
      "add_pages":{
         "start":"class=\"img-responsive\"",
         "token1":"https://mangabook.org/uploads/manga/",
         "token2":"'",
         "end":"<div id=\"ppp\"",
         "replace":{
            "prefix":"https://mangabook.org/uploads/manga/"
         }
      }
   },
   "test":[
      {
         "proc":"manga_list_complete",
         "assert":{
            "mangas":[
               {
                  "title":"Boku Girl / Я девушка!",
                  "link":"https://mangabook.org/manga/boku-girl",
                  "uniq":"boku-girl"
               }
            ],
            "mangas_count":">=50"
         }
      },
      {
         "proc":"manga_complete",
         "link":"https://mangabook.org/manga/bleach",
         "title":"Bleach / Блич",
         "assert":{
            "author":"@Kubo Taito",
            "cover":"@https://mangabook.org/uploads/manga/bleach/cover/cover_250x350.jpg",
		    "summary":"@^(Жил-был парень по имени Ичиго Куросаки)",
            "genres":"@scifi",
            "status":"unknown",
            "genres_count":">=2",
            "chapters_count":">=700",
            "chapters":[
               {
                  "folder":"Vol.1-Ch.1/",
                  "link":"https://mangabook.org/manga/bleach/Vol.1-Ch.1",
                  "title":"Глава №1"
               },
               {
                  "link":"https://mangabook.org/manga/bleach/Vol.1-Ch.2",
                  "title":"Глава №2"
               }
            ]
         }
      },
      {
         "proc":"chapter_complete",
         "link":"https://mangabook.org/manga/bleach/Vol.1-Ch.1",
         "title":"Глава №1",
         "assert":{
            "pages":[
               {"image": "https://mangabook.org/uploads/manga/bleach/chapters/Vol.1-Ch.1/001.png"},
               {"image": "https://mangabook.org/uploads/manga/bleach/chapters/Vol.1-Ch.1/002.jpg"},
               {"image": "https://mangabook.org/uploads/manga/bleach/chapters/Vol.1-Ch.1/003.jpg"}
            ],
            "pages_count":"55"
         }
      }
   ]
}