{
	"id": 40155,
	"version": 50,
	"title": "Re-Manga!",
	"host": "https://remanga.org",
	"public_link": "https://remanga.org/manga",
	"folder": "rbgmanga",
	"name" : "rbgmanga",
	"chapters_order": true,
	"language": "ru",
	"encoding": "utf-8",
	"icon_link": "https://fant.my1.ru/mwparser/mwx/img/rbgmanga.png",
	"self_link": "https://fant.my1.ru/mwparser/mwx/json/rbgmanga.json",
	"catalog_link": "https://fant.my1.ru/mwparser/mwx/catalog/rbgmanga.p",
    "author": {"name": "Veopot","email": "fant-net@ya.ru"},


    "manga_search_complete": {
	    "headers": {
			"Accept": "*/*",
			"Referer": "https://remanga.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		"search_link": "https://api.remanga.org/api/search/?count=10&page=$page$&query=$query$",
		"query_type": "GET",
		
		
		"add_manga":  {
			"start": "content\"",
			"end": "props\"",
            "next": "en_name\"",
			
			"link":  { 
			"token1": "dir\":\"", "token2": "\"",
			"replace": {"prefix": "https://remanga.org/manga/"} },
			
			"cover":  { 
			"token1": "dir\":\"", "token2": "\"",
			"replace": [{"prefix": "https://api.remanga.org/media/titles/"},
                        {"sufix": "/mid_cover.jpg"} ]},
			
			"title": { "before": "rus_name",
			"token1": "en_name\":\"", "token2": "\""},
			
			"uniq": { "before": "rus_name",
			"token1": "en_name\":\"", "token2": "\""},
			
			"additional_title": {
			"token1": "rus_name\":\"", "token2": "\""}
		
			
			
	}},

	"manga_list_complete": {
	   "headers": {
			"Accept": "*/*",
			"Referer": "https://remanga.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},

	  "iterator": {
		"base_url": "https://remanga.org/manga?ordering=-id&page=",
		"append_nums": { "from": 1, "to": 300 }
		},
		"add_manga": {
		    "start": "catalog\"",
			"end": "chapter\"",
            "next": "en_name\"",
			
			"link":  { 
			"token1": "dir\":\"", "token2": "\"",
			"replace": {"prefix": "$scheme$//$hostname$/manga/"} },
			
			"cover":  { 
			"token1": "dir\":\"", "token2": "\"",
			"replace": [{"prefix": "https://api.remanga.org/media/titles/"},
                        {"sufix": "/mid_cover.jpg"} ]},
			
			"title": { "before": "rus_name",
			"token1": "en_name\":\"", "token2": "\""},
			
			"uniq": { "before": "rus_name",
			"token1": "en_name\":\"", "token2": "\""},
			
			"additional_title": {
			"token1": "rus_name\":\"", "token2": "\""}
		    }
		
	},

	
	
	"manga_complete": {
	    "headers": {
			"Accept": "*/*",
			"Referer": "https://remanga.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},

	    "title":{ 
		"token1":"en_name\":\"", "token2": "\","},
		
		
		"additional_title":{ 
		"token1":"rus_name\":\"", "token2": "\","},
		
		"uniq":{ 
		
		"token1":"en_name\":\"", "token2": "\","},
		
	 "cover":{
         "after":"property=\"og:image\"",
         "token1":"content=\"",
         "token2":"\""
      },
		
		
		
		
		"summary":{ 
		
		"token1":"description\":\"", "token2": "\","},
		
		"status": { 
		  	"complete": { 
			 "start": "content",
             "end": "similarManga",
			   "values": [ "Закончен", "Переведена", "Сингл"] },
			
            "ongoing": { 
			 "start": "content",
             "end": "similarManga",
                "value": "Продолжается" }
			 },
		
		"chapters":{ 
		"start": "content",
        "end": "similarManga",
		"token1":"total_chapters\":", "token2":","},
		
		
		"add_genres":   {
        "start": "genres\":",
		"end": "],",
		"token1": "name\":\"",
		"token2": "\""
		},
		
		
		"is_mature":   { "values": [">Сэйнэн<", ">Эротика<", ">Юри<", ">Яой<"] },
		
		
		"chapters_from_page": 
		
		{ 
		"after": "branches\"", "token1": "id\":", "token2": ",",
		"replace" : { "prefix" : "https://api.remanga.org/api/titles/chapters/?branch_id=" }
		},
		
		
		"add_chapter": {
            "start": "content\"",
            "end": "],",
            "next": "{\"",
			
            "link": {
                "token1": "id\":",
                "token2": ",",
                "replace": {
                        "prefix": "https://api.remanga.org/api/titles/chapters/"
                }                  
            },
			"uniq": {
                "token1": ",\"to",
                "token2": "\",\"name\"",
				"replace": [
                        { "match" : "me\":",  "text" : "Том_" },
						{ "match" : ",\"chapter\":\"",  "text" : "_Глава_" }
						]
            },
            
            "title": {
                "token1": ",\"to",
                "token2": "\",\"name\"",
				"replace": [
                        { "match" : "me\":",  "text" : "Том_" },
						{ "match" : ",\"chapter\":\"",  "text" : "_Глава_" }
						
						]
            }
        }
	 
},


	"chapter_complete": {
	    "add_numeration": true,
	    "headers": {
			"Accept": "*/*",
			"Referer": "https://remanga.org/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		"add_images": {
			"start": "content",
			"token1": "link\":\"",
			"token2": "\"",
			 "end": "props"
		}
	},
	"test": [
		{
			"proc" : "manga_list_complete",
			   "assert" : {
				"mangas" : [
					{ "title" : "Да здравствует призыв!",
					 "link" : "https://remanga.org/manga/long_live_summons/",
					  "uniq" : "long_live_summons" }
				],
				"mangas_count" : ">=20"
			}
		},
		{
			"proc" : "manga_complete",
			"link" : "https://remanga.org/manga/switch_girl",
